
companies = "test/data/ASXListedCompanies.csv";
output    = "test/output/02-04-Transform.matryo";

main
 = load companies
 & on /row ( rename-fields    ['name, 'symbol, 'grouping] 
           @ group 'grouping)
 & sample 20
 & flatten
 & on /tuples (permute-fields ['symbol, 'name, 'grouping])
 & store output;


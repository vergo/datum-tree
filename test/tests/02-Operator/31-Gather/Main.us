

companies 
 = "test/data/csv/ASXListedCompanies.csv";

main
 = load companies
 & on /row
        $ rename-fields ['name, 'symbol, 'grouping] 
        @ group 'grouping

 & on /grouping/row  
        $ sample 10

 & gather /grouping/row

 & on /row
        $ permute-fields ['symbol, 'name, 'grouping];
        




companies = "test/data/ASXListedCompanies.csv";
output    = "test/output/02-05-Gather.matryo";

main
 = load companies
 & on /row      $ rename-fields ['name, 'symbol, 'grouping] 
                @ group 'grouping
 & on /row/row  $ sample 10     & gather /row/row
 & on /row      $ group 'name   & gather /row/row
 & on /row      $ group 'symbol & gather /row/row
 & store output;


